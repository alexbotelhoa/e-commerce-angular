{
    "[javascript, typescript]": {
      "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
      },
      "eslint.alwaysShowStatus": true,
      "eslint.validate": [
        "javascript",
        "typescript"
      ]
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "files.exclude": {
      "coverage/": true,
    },
    "search.exclude": {
      "**/dist": true
    },
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true,
      "**/dist": true
    }
  }
  